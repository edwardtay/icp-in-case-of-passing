{
  "version": 1,
  "canisters": {
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/download/release-2025-11-21/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/download/release-2025-11-21/internet_identity_dev.wasm.gz",
      "init_arg": "(null)"
    },
    "deadman_switch": {
      "type": "rust",
      "package": "deadman_switch",
      "candid": "deadman_switch/src/deadman_switch.did",
      "build": "cargo build --target wasm32-unknown-unknown --package deadman_switch --release",
      "wasm": "target/wasm32-unknown-unknown/release/deadman_switch.wasm"
    }
  },
  "networks": {
    "testnet": {
      "providers": ["https://testnet.dfinity.network"],
      "type": "persistent"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  }
}

